{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": true,
		"removeComments": true,
		"sourceMap": true,
		"outDir": "dist/",
		"allowSyntheticDefaultImports": false,
		"strict": true,
		// "downlevelIteration": true,
		"isolatedModules": true,
		"target": "ESNext",
		"lib": [ "ES6", "ESNext", ],
		"incremental": true,
	},
	"include": [
		"src/**/*.ts"
	]
}
